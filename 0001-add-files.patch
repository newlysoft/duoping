diff --git a/models/base.go b/models/base.go
new file mode 100644
index 0000000..706fd7e
--- /dev/null
+++ b/models/base.go
@@ -0,0 +1,34 @@
+package models
+
+import (
+	"crypto/md5"
+	"fmt"
+
+	"github.com/astaxie/beego"
+	"github.com/astaxie/beego/orm"
+	_ "github.com/go-sql-driver/mysql"
+)
+var 
+
+func init() {
+	dbhost := beego.AppConfig.String("dbhost")
+	dbport := beego.AppConfig.String("dbport")
+	dbuser := beego.AppConfig.String("dbuser")
+	dbpassword := beego.AppConfig.String("dbpassword")
+	dbname := beego.AppConfig.String("dbname")
+	if dbport == "" {
+		dbport = "3306"
+	}
+	dburl := dbuser + ":" + dbpassword + "@tcp(" + dbhost + ":" + dbport + ")/" + dbname + "?charset=utf8"
+	orm.RegisterDataBase("default", "mysql", dburl)
+	//orm.RegisterModel(new(User), new(Post), new(Tag), new(Option), new(TagPost), new(Mood), new(Photo), new(Album), new(Link))
+	if beego.AppConfig.String("runmode") == "dev" {
+		orm.Debug = true
+	}
+}
+
+func Md5(buf []byte) string {
+	hash := md5.New()
+	hash.Write(buf)
+	return fmt.Sprintf("%x", hash.Sum(nil))
+}
diff --git a/models/user.go b/models/user.go
new file mode 100644
index 0000000..7b2ae98
--- /dev/null
+++ b/models/user.go
@@ -0,0 +1,56 @@
+package models
+
+import (
+	"time"
+
+	"github.com/astaxie/beego/orm"
+)
+
+//ç”¨æˆ·è¡¨æ¨¡åž‹
+type User struct {
+	Id         int64
+	Username   string    `orm:"unique;size(15)"`
+	Password   string    `orm:"size(32)"`
+	Email      string    `orm:"size(50)"`
+	Lastlogin  time.Time `orm:"auto_now_add;type(datetime)"`
+	Logincount int64
+	Lastip     string `orm:"size(32)"`
+	Authkey    string `orm:"size(10)"`
+	Active     int8
+}
+
+func (m *User) TableName() string {
+	return TableName("user")
+}
+
+func (m *User) Insert() error {
+	if _, err := orm.NewOrm().Insert(m); err != nil {
+		return err
+	}
+	return nil
+}
+
+func (m *User) Read(fields ...string) error {
+	if err := orm.NewOrm().Read(m, fields...); err != nil {
+		return err
+	}
+	return nil
+}
+
+func (m *User) Update(fields ...string) error {
+	if _, err := orm.NewOrm().Update(m, fields...); err != nil {
+		return err
+	}
+	return nil
+}
+
+func (m *User) Delete() error {
+	if _, err := orm.NewOrm().Delete(m); err != nil {
+		return err
+	}
+	return nil
+}
+
+func (m *User) Query() orm.QuerySeter {
+	return orm.NewOrm().QueryTable(m)
+}
